{
	// Style data id
	"id": "leveltesting",

	// Hue options
	"hue_min": 0,
	"hue_max": 360,
	"hue_ping_pong": false,
	"hue_increment": 0.7,
	
	// Pulse options
	"pulse_min": 0.0,
	"pulse_max": 1.5,
	"pulse_increment": 0.025,

	// 3D options
	"3D_depth": 8,
	"3D_skew": 1,
	"3D_spacing": 3,
	"3D_darken_multiplier": 15, //this is how fast the 3d layers darken as you go down
	"3D_alpha_multiplier": 0.5, //this is how fast the 3d layers colour fades as you go down
	"3D_alpha_falloff": 30, //this is how fast the 3d layers go transparent as you go down
	//"3D_override_color": "main",
	"3D_pulse_min": 2,
	"3D_pulse_max": 2,
	"3D_pulse_speed": 0.01,

	// Notes
	//going too high with alpha falloff will cause it to reset to default
	
	// Main color
	"main": { "main": true, "dynamic": true, "value": [255, 0, 0, 255], "pulse": [-80, 75, 65, 0] },
	//in RGBA form, RGB goes 0-255, A goes 0-1, anything higher will default to 1

	// Background colors
	"colors":
	[
		{ "dynamic": true, "dynamic_offset": false, "dynamic_darkness": 2.7, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 0]},
		{ "dynamic": true, "dynamic_offset": false, "dynamic_darkness": 3.5, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 0]}
	]
}