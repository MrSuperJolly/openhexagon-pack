{
	// Style data id
	"id": "sawblade",

	// Hue options
	"hue_min": 0,
	"hue_max": 0,
	"hue_ping_pong": false,
	"hue_increment": 0,
	
	// Pulse options
	"pulse_min": 0.0,
	"pulse_max": 1.5,
	"pulse_increment": 0.025,

	// 3D options
	"3D_depth": 6,
	"3D_skew": 0.4,
	"3D_spacing": 250,
	"3D_darken_multiplier": 1.5, //this is how fast the 3d layers darken as you go down
	"3D_alpha_multiplier": 100, //this is how fast the 3d layers colour fades as you go down
	"3D_alpha_falloff": 0.15, //this is how fast the 3d layers go transparent as you go down
	//"3D_override_color": "main",
	//"3D_pulse_min": 0,
	//"3D_pulse_max": 0.2,
	"3D_pulse_speed": 0,
	
	// Main color
	"main": { "main": true, "dynamic": true, "value": [255, 255, 255, 255], "pulse": [-255, -255, -255, -255] },

	// Background colors
	"colors":
	[
		{ "dynamic": false, "dynamic_offset": false, "dynamic_darkness": 1.0, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 40]},
		{ "dynamic": false, "dynamic_offset": false, "dynamic_darkness": 1.0, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 80]},
		{ "dynamic": false, "dynamic_offset": false, "dynamic_darkness": 1.0, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 120]},
		{ "dynamic": false, "dynamic_offset": false, "dynamic_darkness": 1.0, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 160]},
		{ "dynamic": false, "dynamic_offset": false, "dynamic_darkness": 1.0, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 240]},
		{ "dynamic": false, "dynamic_offset": false, "dynamic_darkness": 1.0, "value": [0, 0, 0, 0], "pulse": [0, 0, 0, 255]}
	]
}